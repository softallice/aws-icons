{
  "name": "aws-icons",
  "version": "1.0.0",
  "description": "AWS Architecture Icons as React Components - Optimized with tree-shaking support",
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/aws-icons.git"
  },
  "keywords": [
    "aws",
    "icons",
    "react",
    "components",
    "architecture",
    "svg",
    "aws-icons"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./dynamic": {
      "types": "./dist/dynamic.d.ts",
      "import": "./dist/esm/dynamic.js",
      "require": "./dist/cjs/dynamic.js"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "pnpm clean && pnpm generate && rollup -c",
    "clean": "rm -rf dist",
    "generate": "tsx scripts/build-icons.ts",
    "prebuild": "npm run generate",
    "dev": "npm run generate -- --watch"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "camelcase": "^6.3.0",
    "cheerio": "^1.0.0-rc.12",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "react": "^18.2.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "dependencies": {
    "clsx": "^2.1.0"
  }
}
